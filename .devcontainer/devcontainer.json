// https://code.visualstudio.com/docs/remote/devcontainerjson-reference
// https://mcr.microsoft.com/v2/vscode/devcontainers/typescript-node/tags/list
{
  "image": "mcr.microsoft.com/vscode/devcontainers/typescript-node:16-bullseye",
  "extensions": [
    "tomsaunders-code.workspace-explorer",
  ],
  "settings": {
    "workspaceExplorer.workspaceStorageDirectory": "/workspaces",
  },
  "postCreateCommand": "/bin/bash /workspaces/devcontainer-ts/.devcontainer/post-create.sh"
}
