// https://code.visualstudio.com/docs/remote/devcontainerjson-reference
{
  "image": "mcr.microsoft.com/vscode/devcontainers/typescript-node",
  "workspaceFolder": "/workspaces",
  "extensions": [
    "tomsaunders-code.workspace-explorer",
    "dbaeumer.vscode-eslint",
    "esbenp.prettier-vscode",
  ],
  "settings": {
    "workspaceExplorer.workspaceStorageDirectory": "/workspaces",
  },
  "postCreateCommand": "/bin/bash /workspaces/ts-node-devcontainer/.devcontainer/post-create.sh"
}
